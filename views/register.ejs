<h1>Register</h1>
<form action="/user/register" method="POST">
    <div>
        <label for="firstName">First Name</label>
        <input type="text" id="firstName" name="firstName" required>
    </div>
    <div>
        <label for="lastName">Last Name</label>
        <input type="text" id="lastName" name="lastName" required>
    </div>
    <div>
        <label for="emai">Email</label>
        <input type="email" id="email" name="email" required>
    </div>
    <div>
        <label for="password">Password</label>
        <input type="password" id="password" name="password" requred>
    </div>
    <p>Billing Address</p>
    <div>
        <label for="billingStreet1">Street 1</label>
        <input type="text" id="billingStreet1" name="billingAddress.street1">
    </div>
    <div>
        <label for="billingStreet2">Street 2</label>
        <input type="text" id="billingStreet2" name="billingAddress.street2">
    </div>
    <div>
        <label for="billingCity">City</label>
        <input type="text" id="billingCity" name="billingAddress.city">
    </div>
    <div>
        <label for="billingState">State</label>
        <input type="text" id="billingState" name="billingAddress.state">
    </div>
    <div>
        <label for="billingZipcode">Zipcode</label>
        <input type="text" id="billingZipcode" name="billingAddress.zipcode">
    </div>
    <p>Shipping Address</p>
    <div>
        <label for="shippingStreet1">Street 1</label>
        <input type="text" id="shippingStreet1" name="shippingAddress.street1">
    </div>
    <div>
        <label for="shippingStreet2">Street 2</label>
        <input type="text" id="shippingStreet2" name="shippingAddress.street2">
    </div>
    <div>
        <label for="shippingCity">City</label>
        <input type="text" id="shippingCity" name="shippingAddress.city">
    </div>
    <div>
        <label for="shippingState">State</label>
        <input type="text" id="shippingState" name="shippingAddress.state">
    </div>
    <div>
        <label for="shippingZipcode">Zipcode</label>
        <input type="text" id="shippingZipcode" name="shippingAddress.zipcode">
    </div>
    <button type="submit">Register</button>
</form>

<a href="/user/login">Login</a>